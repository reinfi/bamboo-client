{
  "name": "reinfi/bamboo-client",
  "description": "PHP Bamboo Client to access REST Api.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Martin Reinfandt",
      "email": "reinfandt@z-em.de"
    }
  ],
  "homepage": "https://github.com/reinfi/bamboo-client",
  "support": {
    "issues": "https://github.com/reinfi/bamboo-client/issues",
    "source": "https://github.com/reinfi/bamboo-client"
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.29.0",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.9.11",
    "phpstan/phpstan-deprecation-rules": "^1.1.1",
    "phpstan/phpstan-phpunit": "^1.3.3",
    "phpstan/phpstan-strict-rules": "^1.4.4",
    "phpunit/phpunit": "^9.5.27"
  },
  "autoload": {
    "psr-4": {
      "Reinfi\\BambooClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Reinfi\\BambooClient\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true
    },
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
